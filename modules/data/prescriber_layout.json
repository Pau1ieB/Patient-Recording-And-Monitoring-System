            {
                "type":"div",
                "attr":{"id":"registration"},
                "classes":["button","margin_top","mouse"],
                "content":[
                    {
                        "type":"div",
                        "classes":["button_image","image_user"]
                    },
                    {
                        "type":"div",
                        "classes":["display_inline","margin_left_half"],
                        "text":"Patient Registration"
                    }
                ]
            },
            {
                "type":"div",
                "attr":{"id":"monitoring"},
                "classes":["button","margin_top","mouse"],
                "content":[
                    {
                        "type":"div",
                        "classes":["button_image","image_user"]
                    },
                    {
                        "type":"div",
                        "classes":["display_inline","margin_left_half"],
