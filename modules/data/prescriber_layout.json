            {
                "type":"div",
                "attr":{"id":"details"},
                "classes":["button","margin_top","mouse"],
                "content":[
                    {
                        "type":"div",
                        "classes":["button_image","image_user"]
                    },
                    {
                        "type":"div",
                        "classes":["display_inline","margin_left_half"],
                        "text":"Details"
                    }
                ]
            },
            {
                "type":"div",
                "attr":{"id":"contact_div"},
                "classes":["display_flex","flex_col","margin_left","width_sub","hidden"],
                "content":[
                    {
                        "type":"input",
                        "attr":{"id":"mobile","type":"text","placeholder":"Contact No."},
                        "classes":["width_full","height_2_5","margin_top_half","font_inherit","pad_h_half"]
                    },
                    {
                        "type":"div",
                        "attr":{"id":"save_contact_button"},
                        "classes":["button","margin_top_half","mouse"],
                        "content":[
                            {
                                "type":"div",
                                "classes":["button_image","image_check"]
                            },
                            {
                                "type":"div",
                                "classes":["display_inline","margin_left_half"],
                                "text":"Save"
                            }
                        ]
                    }
                ]
            },
            {
                "type":"div",
                "attr":{"id":"register"},
                "classes":["button","margin_top","mouse"],
                "content":[
                    {
                        "type":"div",
                        "classes":["button_image","image_update"]
                    },
                    {
                        "type":"div",
                        "classes":["display_inline","margin_left_half"],
                        "text":"Register"
                    }
                ]
            },
            {
                "type":"hr",
                "classes":["margin_top_double","width_full","height_0_2"]
            },
            {
                "type":"div",
                "attr":{"id":"exit"},
                "classes":["button","margin_top_double","mouse"],
                "content":[
                    {
                        "type":"div",
                        "classes":["button_image","image_exit"]
                    },
                    {
                        "type":"div",
                        "classes":["display_inline","margin_left_half"],
                        "text":"Back"
                    }
                ]
            }
        ],
        "buttons":[
            {"id":"add_therapy","func":"OpenTherapy","args":"false"},
            {"id":"select_therapy_button","func":"SelectTherapy","args":"false"},
            {"id":"add_notifications","func":"OpenPatientNotification"},
            {"id":"details","func":"ShowHiddenDiv","value":"contact_div"},
            {"id":"save_contact_button","func":"SaveContact"},
            {"id":"register","func":"RegisterPatient"},
            {"id":"exit","func":"ExitRegistration"}
