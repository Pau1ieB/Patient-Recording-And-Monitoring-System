                "attr":{"id":"select_therapy_div"},
                "classes":["display_flex","flex_col","margin_left","width_sub","hidden"],
                "content":[
                    {
                        "type":"select",
                        "attr":{"id":"select_therapy"},
                        "classes":["width_full","height_2_5","font_inherit","background_white","margin_top_half"],
                        "content":[]
