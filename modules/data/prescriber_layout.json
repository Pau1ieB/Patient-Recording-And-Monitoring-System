                "classes":["button","margin_top_double","mouse"],
                "content":[
                    {
                        "type":"div",
                        "classes":["button_image","image_exit"]
                    },
                    {
                        "type":"div",
                        "classes":["display_inline","margin_left_half"],
                        "text":"Logout"
                    }
                ]
            }            
        ],
        "buttons":[
            {"id":"registration","func":"OpenPatientRegistration"},
            {"id":"monitoring","func":"OpenPatientMonitoring"},
            {"id":"logout","func":"Logout"}
        ]
